<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.edwin.appmyevents.interfaz.Ingreso">

    <Button
        android:id="@+id/btnLoginPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btnRegistroPrincipal"
        android:layout_alignEnd="@+id/imageView4"
        android:layout_marginBottom="94dp"
        android:layout_marginEnd="46dp"
        android:text="Ingreso" />

    <Button
        android:id="@+id/btnRegistroPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/btnLoginPrincipal"
        android:layout_marginBottom="87dp"
        android:text="Registro" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_above="@+id/btnLoginPrincipal"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="31dp"
        app:srcCompat="@mipmap/logo" />

    <Button
        android:id="@+id/btnImagenes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnRegistroPrincipal"
        android:layout_marginEnd="19dp"
        android:layout_toStartOf="@+id/btnRegistroPrincipal"
        android:text="Imagenes" />
</RelativeLayout>
